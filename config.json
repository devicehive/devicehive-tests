{
  "server": {
    "wsUrl": "ws://localhost:8080/websocket",
    "ip": "127.0.0.1",
    "port": "8080",
    "restUrl": "http://localhost:8080/dh/rest",
    "jwt": "admin_token"
  },
  "loadTests": {
    "runAsync": false,
    "tests": [
      {
        "type": "rest-message",
        "disabled": false,
        "name": "30 REST notifications, 1 device",
        "senders": 30,
        "requestsPerSender": 10,
        "intervalMillis": 200,
        "deviceIds": [ "e50d6085-2aba-48e9-b1c3-73c673e414be" ],
        "names": [ "a-first", "a-second" ],
        "requests": [
          {
            "method": "POST",
            "type": "notification",
            "data": {
              "notification": "{#name}",
              "parameters": {
                "t": "20.0",
                "p": "29.92"
              }
            }
          }
        ],
        "waitDelay": 1000
      },
      {
        "type": "rest-message",
        "disabled": false,
        "name": "30 REST commands, 1 device",
        "senders": 30,
        "requestsPerSender": 10,
        "intervalMillis": 200,
        "deviceIds": [ "e50d6085-2aba-48e9-b1c3-73c673e414be" ],
        "names": [ "a-cmd-first", "a-cmd-second" ],
        "requests": [
          {
            "method": "POST",
            "type": "command",
            "data": {
              "command": "{#name}",
              "parameters": {
                "enabled": false
              }
            }
          }
        ],
        "waitDelay": 1000
      },
      {
        "type": "ws-message",
        "disabled": false,
        "name": "20 connections, 3 notifications, 1 device",
        "connections": 20,
        "messagesPerClient": 3,
        "intervalMillis": 100,
        "deviceIds": [ "e50d6085-2aba-48e9-b1c3-73c673e414be" ],
        "names": [ "a-first", "a-second", "a-third", "a-fourth", "a-fifth", "a-sixth" ],
        "messages": [
          {
            "action": "notification/insert",
            "deviceId": "{#deviceId}",
            "notification": {
              "notification": "{#name}",
              "parameters": {
                "t" : "20.0",
                "p" : "29.92"
              }
            },
            "requestId": "{#requestId}"
          }
        ],
        "waitDelay": 1000
      },
      {
        "type": "ws-message",
        "disabled": false,
        "name": "20 connections, 5 notifications, 99999999 devices",
        "connections": 20,
        "messagesPerClient": 5,
        "intervalMillis": 100,
        "devices": 1000000,
        "deviceIds": "aaaaaaaa-aaaa-0000-0-000000a{#}",
        "names": [ "b-first", "b-second", "b-third", "b-fourth", "b-fifth", "b-sixth" ],
        "messages": [
          {
            "action": "notification/insert",
            "deviceId": "{#deviceId}",
            "notification": {
              "notification": "{#name}",
              "parameters": {
                "t" : "20.0",
                "p" : "29.92"
              }
            },
            "requestId": "{#requestId}"
          }
        ],
        "waitDelay": 1000
      },
      {
        "type": "ws-message",
        "disabled": false,
        "name": "20 clients, 10 commands, 1 device",
        "connections": 20,
        "messagesPerClient": 10,
        "intervalMillis": 100,
        "deviceIds": [ "e50d6085-2aba-48e9-b1c3-73c673e414be" ],
        "names": [ "a-cmd-first", "a-cmd-second", "a-cmd-third", "a-cmd-fourth", "a-cmd-fifth", "a-cmd-sixth" ],
        "messages": [
          {
            "action": "command/insert",
            "deviceId": "{#deviceId}",
            "command": {
              "command": "{#name}",
              "parameters": {
                "t" : "on",
                "p" : "off"
              }
            },
            "requestId": "{#requestId}"
          }
        ],
        "waitDelay": 1000
      },
      {
        "type": "ws-notification",
        "disabled": false,
        "name": "Full cycle: 4 devices, 10 notifications, 1 client",
        "clients": 1,
        "devices": 4,
        "notifsPerDevice": 10,
        "intervalMillis": 100,
        "listenAllDevices": false,
        "deviceIds": "aaaaaaaa-aaaa-0000-0-000000a{#}",
        "notifications": [ "a-first", "a-second", "a-third", "a-fourth", "a-fifth", "a-sixth" ],
        "parameters": {
          "t" : "20.0",
          "p" : "29.92"
        },
        "waitDelay": 3000
      },
      {
        "type": "ws-notification",
        "disabled": false,
        "name": "Full cycle: 1 device, 2 notifications, 4 clients",
        "clients": 4,
        "notifsPerDevice": 2,
        "intervalMillis": 200,
        "listenAllDevices": true,
        "deviceIds": [ "e50d6085-2aba-48e9-b1c3-73c673e414be" ],
        "notifications": [ "b-first", "b-second", "b-third", "b-fourth", "b-fifth", "b-sixth" ],
        "parameters": {
          "t" : "20.0",
          "p" : "29.92"
        },
        "waitDelay": 3000
      },
      {
        "type": "ws-command",
        "disabled": false,
        "name": "Full cycle: 1 client, 100 commands, 4 devices",
        "clients": 1,
        "devices": 4,
        "commandsPerClient": 100,
        "intervalMillis": 100,
        "sendToAllDevices": false,
        "deviceIds": "aaaaaaaa-aaaa-0000-0-000000a{#}",
        "commands": [ "a-first", "a-second", "a-third", "a-fourth", "a-fifth", "a-sixth" ],
        "parameters": {
          "t" : "20.0",
          "p" : "29.92"
        },
        "waitDelay": 3000
      },
      {
        "type": "ws-command",
        "disabled": false,
        "name": "Full cycle: 4 clients, 2 commands, 1 device",
        "clients": 4,
        "commandsPerClient": 2,
        "intervalMillis": 200,
        "sendToAllDevices": false,
        "deviceIds": [ "e50d6085-2aba-48e9-b1c3-73c673e414be" ],
        "commands": [ "b-cmd-first", "b-cmd-second", "b-cmd-third", "b-cmd-fourth", "b-cmd-fifth", "b-cmd-sixth" ],
        "parameters": {
          "t" : "on",
          "p" : "off"
        },
        "waitDelay": 3000
      }
    ]
  },
  "log": {
    "level": "SUMMARY"
  }
}